  "description": "",
  "main": "./dist/cli.js",
  "bin": {
    "llm": "./dist/cli.cjs"
  },
  "scripts": {
    "build": "pnpm clean && esbuild --bundle --sourcemap --bundle --platform=node --outfile=dist/cli.cjs ./src/cli.ts",
